<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@android:color/white"
    android:layout_height="match_parent"
    tools:context=".Activities.Fragments.VendorsParent">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fl" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

<com.aurelhubert.ahbottomnavigation.AHBottomNavigation            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:id="@+id/botton_nav_bar"
            app:itemIconTint="@android:color/black"/>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fab"
            android:src="@drawable/baseline_add_black_48"
            app:fabSize="normal"
            app:backgroundTint="@android:color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@id/botton_nav_bar"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="0dp"
            android:layout_margin="16dp"/>
    </RelativeLayout>
</FrameLayout>